OKMSG	.BYT $D,'OK',$D,$0
ERR	.BYT $D,' ERROR',0 ;ADD A C/R FOR VIXEN SCREEN
INTXT	.BYT ' IN ',0
REDDY	.BYT $D,$A,'READY.',$D,$A,0
